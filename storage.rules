rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    function signedIn() {
      return request.auth != null;
    }

    match /profile_images/{file} {
      allow read, write: if signedIn();
    }

    match /notification_images/{file} {
      allow read, write: if signedIn();
    }
  }
}
